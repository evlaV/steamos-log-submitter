[Unit]
Description=SteamOS Offload - /var/lib/steamos-log-submitter

[Mount]
What=@LOCALSTATEDIR@/lib/steamos-log-submitter
Where=/var/lib/steamos-log-submitter
Type=none
Options=bind

[Install]
WantedBy=steamos-offload.target
